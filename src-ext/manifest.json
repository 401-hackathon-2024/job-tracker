{
    "manifest_version": 3,
    "name": "Auto Job Application Filler",
    "version": "1.0",
    "description": "Autofills job application forms.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}